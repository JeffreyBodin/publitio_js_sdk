{
  "name": "publitio_js_sdk",
  "version": "1.0.10",
  "description": "Simple JavaScript SDK for Publitio API",
  "main": "build/publitio-api.js",
  "scripts": {
    "build": "babel --source-maps=true src -d build && webpack && cpy index.d.ts build --rename=publitio-api.min.d.ts",
    "prepublishOnly": "npm run build",
    "watch": "watch 'npm run build' src",
    "lint": "eslint src examples --fix",
    "demo-server": "node demo/simple-server.js",
    "generate-docs": "typedoc --mode file --readme none --out docs --includeDeclarations --excludeExternals --externalPattern **/node_modules/**/*.* index.d.ts"
  },
  "keywords": [
    "publitio",
    "api",
    "javascript",
    "js"
  ],
  "author": "",
  "license": "",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/node": "^12.7.2",
    "babel-loader": "^8.0.6",
    "cpy-cli": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsdoc": "^3.6.3",
    "tsdoc": "0.0.4",
    "typedoc": "^0.15.0",
    "watch": "^1.0.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@types/node": "^12.7.1",
    "axios": "^0.19.0",
    "crypto-js": "^3.1.9-1",
    "form-data": "^2.5.0"
  }
}
